package Test.Action;

import Test.Object.Login;
import Test.Utility.utility;

public class LoginAction {
	public static void input_Email () throws Exception
	{
		Login.txt_UserName().sendKeys(utility.Email);
	}

	
	public static void input_Pass () throws Exception
	{
		Login.txt_Pass().sendKeys(utility.Password);
	}
	
	public static void click_Login () throws Exception
	{
		Login.btn_Login().click();
	}
	
	
}
